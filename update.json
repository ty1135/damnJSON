{
    "id":"CONTAINER",
    "layout":"Boxlayout",
    "component":[
        {
            "version":1,
            "id":"whatfrever",
            "view":{
                "type":"textfield",
                "description":"文本框",
                "props":{
                    "label":"权重",
                    "placeholder":"10"
                }
            },
            "model":{
                "editable":true,
                "text":"8"
            }
        },
        {
            "version":1,
            "id":"BUTTON",
            "view":{
                "type":"button",
                "props":{
                    "label":"更新按钮"
                },
                "model":{
                    "disable":false
                },
                "control":{
                    "click":{
                        "endpoint":"get https://raw.githubusercontent.com/ty1135/damnJSON/master/update_response.json",
                        "payload":{
                            "trigger":{
                                "id":"BUTTON",
                                "method":"click"
                            },
                            "data":[
                                {
                                    "id":"whatfrever",
                                    "value":{
                                        "path":"model.text"
                                    }
                                }
                            ]
                        },
                        "response":{
                            "status":null,
                            "body":[
                                {
                                    "id":"whatfrever",
                                    "value":{
                                        "path":"model.text"
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        }
    ]
}